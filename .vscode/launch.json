{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "[Luaud] Compile and Debug",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceRoot}/build/luaud/luaud.exe",
      "args": [
        "59000",
        "${workspaceRoot}/tests/lua/main.lua"
      ],
      "stopAtEntry": false,
      "environment": [],
      "preLaunchTask": "CMake: build"
    },
    {
      "name": "[Luaud] Debug",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceRoot}/build/luaud/luaud.exe",
      "args": [
        "59000",
        "${workspaceRoot}/tests/lua/main.lua"
      ],
      "stopAtEntry": false,
      "environment": []
    },
    {
      "name": "[Luaud] Attach",
      "type": "cppvsdbg",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}